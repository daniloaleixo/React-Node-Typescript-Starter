  "scripts": {
    "client": "npm run lint && webpack-dev-server --config=configs/webpack/dev.js",
    "serve": "npm run build:server && node dist/server/src/bin/www.js",
    "build": "npm run clean-dist && npm run build:server && npm run build:client",
    "clean-dist": "rm -f -r -d dist",
    "build:client": "npm run lint && ENV=prod webpack -p --config=configs/webpack/prod.js",
    "build:server": "npm run build-ts && npm run lint:ts",
    "build:scripts": "npm run build-ts && npm run lint:ts",
    "test": "npm run test:client && npm run test:server && node scripts/tests/mapCoverage.js",
    "test:client": "jest --coverage --config=configs/jest-client.json",
    "test:server": "jest --coverage --config=configs/jest-server.json",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json",
    "lint:sass": "stylelint ./client/**/*.scss"
  },
